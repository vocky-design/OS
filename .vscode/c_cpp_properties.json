{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/kernel/*",
                "${workspaceFolder}/kernel/device/*",
                "${workspaceFolder}/kernel/thread/*",
                "${workspaceFolder}/kernel/userprog/*",
                "${workspaceFolder}/lib/*",
                "${workspaceFolder}/lib/kernel/*"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cppStandard": "gnu++98",
            "cStandard": "c11",
            "compilerArgs": [
                "-m32",
                "-fno-builtin",
                "-fno-stack-protector",
                "-Wall",
                "-Wstrict-prototypes",
                "-Wmissing-prototypes",
                "-c"
            ],
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}